{
  "name": "react-starter",
  "version": "0.1.8",
  "keywords": [
    "react",
    "gh-pages",
    "typescript",
    "vite",
    "vitest",
    "css modules",
    "front-end",
    "yarn"
  ],
  "homepage": "https://najwer23.github.io/react-starter/",
  "type": "module",
  "scripts": {
    "bench": "vitest bench",
    "build": "tsc --build --clean && vite build",
    "deploy": "yarn run build && gh-pages -d build && rimraf build",
    "god": "ncu -u && yarn install && yarn run start",
    "preview": "yarn build && vite preview --open --host",
    "start": "vite --open --host",
    "test": "vitest",
    "prepare": "husky",
    "check:prettier": "prettier -c '**/*{json,ts,tsx,js,md,css}'",
    "check:lint": "eslint . && stylelint '**/*css'",
    "check:all": "yarn check:prettier && yarn check:lint",
    "fix:prettier": "prettier -c '**/*{json,ts,tsx,js,md,css}' -w",
    "fix:all": "eslint . --fix && stylelint '**/*.css' --fix && yarn fix:prettier"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "rimraf": "^5.0.8",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@4.2.2"
}
